{
  "name": "damcclean/statamic-cookie-notice",
  "type": "statamic-addon",
  "description": "Cookie Notice addon for Statamic",
  "authors": [
    {
      "name": "Duncan McClean",
      "email": "duncan@littlepenguin.dev",
      "homepage": "http://duncanm.dev",
      "role": "Developer at Little Penguin"
    }
  ],
  "autoload": {
    "psr-4": {
      "Damcclean\\CookieNotice\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Damcclean\\CookieNotice\\Tests\\": "tests"
    }
  },
  "extra": {
    "statamic": {
      "name": "Cookie Notice",
      "description": "Display a cookie consent notice on your Statamic site"
    },
    "laravel": {
      "providers": [
        "Damcclean\\CookieNotice\\ServiceProvider"
      ]
    }
  },
  "require": {
    "statamic/cms": "dev-master"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0"
  },
  "minimum-stability": "dev"
}
