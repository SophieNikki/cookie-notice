{
  "name": "doublethreedigital/cookie-notice",
  "description": "Display a cookie consent notice on your Statamic site",
  "authors": [
    {
      "name": "Duncan McClean",
      "email": "duncan@doublethree.digital",
      "homepage": "http://duncanm.dev",
      "role": "Founder of Double Three Digital"
    }
  ],
  "autoload": {
    "psr-4": {
      "DoubleThreeDigital\\CookieNotice\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DoubleThreeDigital\\CookieNotice\\Tests\\": "tests"
    }
  },
  "extra": {
    "statamic": {
      "name": "Cookie Notice",
      "description": "Display a cookie consent notice on your Statamic site"
    },
    "laravel": {
      "providers": [
        "DoubleThreeDigital\\CookieNotice\\ServiceProvider"
      ]
    }
  },
  "require": {
    "statamic/cms": "^3.0@beta"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.1"
  },
  "minimum-stability": "dev",
  "scripts": {
    "lint": [
      "php-cs-fixer fix ./src"
    ],
    "test": [
      "./vendor/bin/phpunit"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
